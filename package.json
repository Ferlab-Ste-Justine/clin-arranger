{
  "name": "clin-arranger",
  "version": "1.1.0",
  "description": "GraphQL API",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -w .env -w src --exec node --experimental-modules=node --es-module-specifier-resolution=node ./src/index.js",
    "start-with-dockers": "(cd ./docker; ./launch.sh); npm run start",
    "stop-dockers": "(cd ./docker; ./teardown.sh);",
    "prod": "cross-env NODE_ENV=production node --experimental-modules=node --es-module-specifier-resolution=node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ferlab-Ste-Justine/clin-arranger.git"
  },
  "author": "Centre de recherche Ste-Justine",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ferlab-Ste-Justine/clin-arranger/issues"
  },
  "homepage": "https://github.com/Ferlab-Ste-Justine/clin-arranger#readme",
  "dependencies": {
    "@arranger/server": "^2.16.1",
    "@elastic/elasticsearch": "^7.15.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "keycloak-connect": "^12.0.3",
    "regenerator-runtime": "^0.13.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
